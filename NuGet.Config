<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Package Source Mapping to satisfy Central Package Management when multiple sources exist.
       We map all packages to nuget.org and intentionally leave other sources (e.g., 'Bufdir GitHub') unmapped,
       so they are ignored unless explicitly mapped later. -->
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
